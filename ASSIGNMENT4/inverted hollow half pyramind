#include<stdio.h>
int main()
{
    int rows,column;
    scanf("%d%d",&rows,&column);
    printf("enter no. of rows and column\n\n");
    printf("\n");
    
    if(rows > 0  && column > 0)
  {  
      for(int i=1; i<=rows; i++)
    { 
    for(int j=i; j<=column; j++)
    {
        if(i==1 || i==rows || j==i || j==column)
        printf("*");
        else
        printf("  ");
}
       printf("\n");
    }
  }
    return 0;
}
