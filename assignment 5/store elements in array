#include <stdio.h>  
  
int  main()  
{  
    int arr[10]; 
    int i;  
      
  
    printf("Input elements in the array :\n");  
    for(i=0; i<10; i++)  
    {  
	    printf(" %d \n ",i);
        scanf("%d", &arr[i]);  
    }  
  
    for(i=0; i<10; i++)  
    {  
        printf(" ", arr[i]);  
    } 
    printf("\n");	
}
