#include <stdio.h>    
     
int main()    
{    
   
    int arr[10],size;     
    
    printf("Original size of array: \n");    
    scanf("%d",&size);
    for (int i = 0; i < size; i++) 
    {     
        printf("%d \n", arr[i]); 

    }      
        
    printf("Array in reverse order: \n");    
    
    for (int i =( size-1); i >= 0; i--)
    {     
        printf(" %d ", arr[i]);     
    }     
    return 0;    
}    


