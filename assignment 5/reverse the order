#include <stdio.h>

int main() {
    int a[100],size;
    printf("enter the size of an array\n");
    scanf("%d",&size);
    
    printf("enter elemnets in an array to reverse order it \n");
    for(int i =0; i< size; i++)
    {
         scanf("%d",&a[i]);
    }
      printf("elemnets in an array are \n");
       for(int i=size -1; i >=0; i--)
    {
         printf("%d   ",a[i]);
    }
    
    return 0;
}
