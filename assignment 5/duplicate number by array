#include <stdio.h>

int main()
{
    int arr[100];
    int j,i,size,count=0;
	
       printf(" the number of elements to be stored  :\n");
       scanf("%d",&size);   
        for(i=0;i<size;i++)
            scanf("%d",&arr[i]); 
        
       for(i=0;i<size;i++)
      
        {
          for(j=i+1;j<size;j++)
            {
	      if(arr[i]==arr[j])
	      {
	          count++;
	          break;
	         
	      }
	      
            }    
    }
       printf("duplicate element %d",count);
}
