#include<stdio.h>
 
int main ()
{
 
  int num1 = 0, num2 = 1, num3, i, n;
  
 printf ("Enter the length of the fibonacci series \n\n");
 
  scanf ("%d", &n);
  printf ("The Fibonacci series is :\n");
 
  printf ("%d\n", num1);
  printf ("%d\n", num2);
  
  for (i = 2; i <= n; i++)
 
    {
 
     num3 = num2 + num1;
 
      printf ("%d\n ", num3);
 
      num1 = num2;
 
      num2 = num3;
 
    }
 
  return 0;
 
}
